+  cmd = env + ' ' + submit + ' ' + cmd + ' ' + postcmd
+  print '[CPU2006]', 'Running \'' + cmd + '\':'
+  print '[CPU2006]', '[---------- Beginning of output ----------]'
+  rc = run(cmd)
+  print '[CPU2006]', '[----------    End of output    ----------]'
+  print '[CPU2006]', 'Done.'
+  return rc
