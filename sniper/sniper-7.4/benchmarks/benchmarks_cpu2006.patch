+EXTRA_FLIBS=
+PORTABILITY_FLIBS=
+LDFFLAGS=
+FOPTIMIZE=
+EXTRA_FOPTIMIZE=
+FPORTABILITY=
+EXTRA_FPORTABILITY=
+BENCH_FFLAGS=
+# Fortran preprocessor
+EXTRA_FPPFLAGS=
+FPPPORTABILITY=
+FPPFLAGS=
+BENCH_FPPFLAGS=
