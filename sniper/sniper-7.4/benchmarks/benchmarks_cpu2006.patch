+      os.chdir(origcwd)
+      if rundir != None:
+        os.system('rm -rf "%s"' % rundir)
+      raise
+
+    rc = run_bm(self.program, cmd_to_run, graphitecmd, env = '', postcmd = postcmd)
+    os.chdir(origcwd)
+    os.system('rm -rf "%s"' % rundir)
+    return rc
